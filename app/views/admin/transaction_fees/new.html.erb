<%# New Transaction Fee %>
<div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-8">
  <div class="flex items-center mb-6">
    <%= link_to admin_transaction_fees_path, class: "text-gray-500 dark:text-gray-400 hover:text-gray-700 dark:hover:text-gray-300 mr-2" do %>
      <%= icon "arrow-left", class: "w-5 h-5" %>
    <% end %>
    <h1 class="text-2xl font-bold text-gray-900 dark:text-white">New Transaction Fee</h1>
  </div>

  <div class="bg-white dark:bg-gray-800 shadow rounded-lg p-6">
    <%= render 'form', transaction_fee: @transaction_fee %>
  </div>
</div>
