<div id="<%= dom_id attendance %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= attendance.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Event:</strong>
    <%= attendance.event_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= attendance.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Checked in at:</strong>
    <%= attendance.checked_in_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Cancelled at:</strong>
    <%= attendance.cancelled_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Additional info:</strong>
    <%= attendance.additional_info %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Form responses:</strong>
    <%= attendance.form_responses %>
  </div>
</div>
