<div id="<%= dom_id ticket_type %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Event:</strong>
    <%= ticket_type.event_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= ticket_type.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= ticket_type.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Price:</strong>
    <%= ticket_type.price %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Quantity:</strong>
    <%= ticket_type.quantity %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Sales start time:</strong>
    <%= ticket_type.sales_start_time %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Sales end time:</strong>
    <%= ticket_type.sales_end_time %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Sold count:</strong>
    <%= ticket_type.sold_count %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Max per user:</strong>
    <%= ticket_type.max_per_user %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Transferable:</strong>
    <%= ticket_type.transferable? ? "Yes" : "No" %>
  </div>
</div>
